{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ],
      "destination": "https://northcromocontrole.com.br/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.northcromocontrole.com.br"
        }
      ],
      "destination": "https://northcromocontrole.com.br/:path*",
      "permanent": true
    }
  ]
}
